<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_catalog">

    <fragment
        android:id="@+id/navigation_catalog"
        android:name="com.example.musicwiki.catalog_fragment"
        android:label="@string/catalog"
        tools:layout="@layout/fragment_catalog" />
    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.example.musicwiki.favorite_fragment"
        android:label="@string/favorite"
        tools:layout="@layout/fragment_favorite" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.musicwiki.profile_fragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile" />
</navigation>